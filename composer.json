{
    "name": "depotwarehouse/blumba",
    "description": "An opinionated EventSourcing/CQRS framework for PHP Projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Troy Pavlek",
            "email": "troy@tpavlek.me"
        }
    ],
    "require": {
        "league/event": "^2.1",
        "illuminate/contracts": "^5.1",
        "nesbot/carbon": "^1.20",
        "ramsey/uuid": "^2.8"
    },
    "suggest": {
        "illuminate/database": "Integrated serialization/connection"
    },
    "require-dev": {
        "illuminate/database": "^5.1",
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Depotwarehouse\\Blumba\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depotwarehouse\\Blumba\\Tests\\": "tests/"
        }
    }
}
